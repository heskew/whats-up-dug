{
  "name": "whats-up-dug",
  "version": "0.1.0",
  "description": "Interactive data exploration CLI for Harper",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "dug": "bin/dug.js"
  },
  "scripts": {
    "build": "bun build src/index.tsx --outdir dist --target node",
    "dev": "bun run src/index.tsx",
    "start": "bun run src/index.tsx",
    "compile": "bun build src/index.tsx --compile --outfile dug",
    "test": "bun test"
  },
  "files": [
    "bin/",
    "dist/",
    "README.md"
  ],
  "keywords": [
    "harper",
    "cli",
    "database",
    "explorer",
    "terminal",
    "tui"
  ],
  "author": "heskew",
  "license": "MIT",
  "dependencies": {
    "@inkjs/ui": "^2.0.0",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "fuse.js": "^7.1.0",
    "ink": "^5.2.0",
    "ink-select-input": "^6.0.0",
    "ink-spinner": "^5.0.0",
    "ink-table": "^3.1.0",
    "ink-text-input": "^6.0.0",
    "react": "^18.3.1",
    "snooplogg": "^6.1.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bun": "^1.2.4",
    "@types/react": "^18.3.18",
    "ink-testing-library": "^4.0.0",
    "typescript": "^5.7.3"
  }
}
